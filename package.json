        "ioredis": "^5.3.2",
        "mysql2": "^3.6.5",
        "node-cron": "^3.0.3",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
