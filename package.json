{
    "name": "mutual-fund-analytics",
    "version": "1.0.0",
    "description": "Mutual Fund Analytics Backend with Redis-based rate limiting and MySQL persistence",
    "main": "src/server.js",
    "type": "module",
    "scripts": {
        "start": "node src/server.js",
        "dev": "node --watch src/server.js",
        "migrate": "node src/db/migrate.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:analytics": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/analytics.test.js"
    },
    "dependencies": {
        "@upstash/redis": "^1.36.1",
